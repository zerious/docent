{
  "name": "docent",
  "version": "0.0.1",
  "description": "A documentation generator for Node.js",
  "dependencies": {
    "mock-fs": "^2.3.1",
    "shellify": "^0.0.4",
    "marked": "~0.3.2",
    "highlight.js": "~8.2.0"
  },
  "devDependencies": {
    "zeriousify": "^0.1.10",
    "exam": "^0.0.8",
    "istanbul": "0.2.9",
    "coveralls": "2.10.0"
  },
  "keywords": [
    "docent"
  ],
  "main": "docent.js",
  "bin": {
    "docent": "docent.js"
  },
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/docent.git",
  "bugs": "https://github.com/lighterio/docent/issues",
  "homepage": "https://github.com/lighterio/docent",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>",
    "Louis Kinley <louis.kinley@gmail.com>"
  ],
  "license": "MIT"
}
